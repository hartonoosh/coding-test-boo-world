  box-shadow: 0 2px 6px rgba(0,0,0,0.03);
  font-weight: 500;
  color: #444;
}
.sort-pill.active {
  background: #4EDCD8;
  color: #fff;
  border: none;
  box-shadow: 0 6px 14px rgba(78,220,216,0.18);
}

/* ---------- Draft (comment form) ---------- */
.comment-draft {
  background: #fff;
  border: 1px solid #f1f1f1;
  border-radius: 25px;
  padding: 16px;
  margin-top: 25px;
  margin-bottom: 25px;
  box-shadow: 1px 0px 14px rgba(0,0,0,0.2);
  position: relative;
  display: none;
}
.comment-draft.show {
  display: block;
  animation: slideDown 240ms ease;
}
@keyframes slideDown {
  from { opacity: 0; transform: translateY(-6px); }
  to { opacity: 1; transform: translateY(0); }
}

/* draft header */
.draft-head {
  display:flex;
  gap:12px;
  align-items:flex-start;
  margin-bottom: 12px;
}
.draft-head img {
  width:48px;
  height:48px;
  border-radius:50%;
  object-fit:cover;
  box-shadow: 0 0 8px rgba(0,0,0,0.7);
}
.draft-head .draft-user {
  font-size: 0.9em;
  font-weight: 500;
}

/* title & body */
.draft-title {
  width:100%;
  border: none;
  font-size: 16px;
  font-weight:600;
  color:#222;
  outline: none;
  padding: 6px 0;
}
.draft-title::placeholder { color: #bdbdbd; font-weight:600; }
.draft-body {
  width:100%;
  min-height:72px;
  border:none;
  resize:vertical;
  outline:none;
  color:#333;
  padding:6px 0;
}

/* vote tags row */
.vote-row {
  display:flex;
  align-items:flex-start;
  gap:12px;
  margin-top: 10px;
  margin-bottom: 12px;
  flex-wrap:wrap;
}
.vote-tag-wrap { 
  position: relative; 
  display:inline-block; 
}
.vote-tag {
  padding: 4px 10px 6px 10px;
  border-radius: 50px;
  border: none;
  cursor: pointer;
  background: rgba(78, 220, 216, 1);
  color: #0a0a0a;
  font-weight: 500;
  opacity: 0.4;
  transition: opacity 160ms ease, background 160ms;
  min-width: 80px;
  text-align: center;
  font-size: 0.8em;
}
.vote-tag.active {
  opacity: 1;
  background: #4EDCD8;
  color: #fff;
  box-shadow: 0 8px 18px rgba(78,220,216,0.14);
}
.vote-tag.dropdown {
  border-radius: 20px 20px 0 0 !important;
}

.vote-tag.ennea {
  background: #13A6A2;
  background-size: cover;
  color: #fff;
}
.vote-tag.ennea.active {
  opacity: 1;
  display: flex;
  gap: 2px;
  align-items: center;
  justify-content: center;
  margin-top: 2px;
}
.vote-tag.ennea.active img{
  width: 18px;
}
.vote-tag.ennea.dropdown {
  border-radius: 20px 20px 0 0 !important;
}

.vote-tag.zodiac {
  background: url(static/space.png) no-repeat center center;
  background-size: cover;
  color: #fff;
}
.vote-tag.zodiac.active {
  opacity: 1;
}
.vote-tag.zodiac.dropdown {
  border-radius: 20px 20px 0 0 !important;
}

/* dropdown yang terikat ke tombol */
.vote-dropdown {
  position: absolute;
  top: calc(100% + 0px); /* 8px jarak dari tombol */
  left: 50%;
  transform: translateX(-50%); /* center relatif ke wrap */
  z-index: 200;
  background: #4EDCD8;
  color: #fff;
  border-radius: 0 0 20px 20px;
  overflow: hidden;
  /* min-width: 120px; */
  /* width: max(140px, 100%); akan minimal 140px atau setara lebar tombol */
  max-height: 320px;
}
.vote-dropdown.ennea {
  background: #13A6A2;
}
.vote-dropdown.zodiac {
  background: url(static/space.png) no-repeat center center;
}

/* list */
.vote-dropdown ul {
  list-style: none;
  margin: 0;
  padding: 5px;
  max-height: 280px;
  /* pastikan ada overflow yang memungkinkan scrolling */
  overflow-y: auto;
  -webkit-overflow-scrolling: touch; /* momentum scrolling di iOS */

  /* sembunyikan scrollbar di berbagai browser */
  scrollbar-width: none;       /* Firefox */
  -ms-overflow-style: none;    /* IE/Edge (legacy) */
  touch-action: auto;          /* pastikan touch gesture diterima */
}

/* Chrome, Safari, Edge Chromium */
.vote-dropdown ul::-webkit-scrollbar {
  width: 0;
  height: 0;
  display: none;
}


/* each item */
.vote-dropdown li {
  padding: 5px 0;
  cursor: pointer;
  font-weight: 600;
  display: flex;
  gap: 2px;
  align-items: center;
  justify-content: center;
  border-radius: 10px;
  font-size: 0.8em;
}

.vote-dropdown li .wing-icon {
  width: 18px;
  height: auto;
  vertical-align: middle;
}

/* hover */
.vote-dropdown li:hover {
  background: rgba(255,255,255,0.08);
}
.vote-dropdown.zodiac li:hover {
  background: rgba(255, 255, 255, 0.2);
}

/* bottom of draft */
.draft-bottom {
  display:flex;
  justify-content:end;
  align-items:center;
  gap:12px;
  margin-top:8px;
}

/* submit button */
.submit-btn {
  background: transparent;
  border: none;
  border-radius: 50%;
  width: 48px;
  height: 48px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  color: #888;
  transition: all 0.25s ease;
}
.submit-btn.enabled {
  color: #4EDCD8;
  background: transparent;
}

.submit-btn svg path {
  transition: filter 0.3s ease;
}
.submit-btn.glow svg path {
  filter: drop-shadow(0 0 6px #4EDCD8);
}
/* disabled */
.submit-btn:disabled {
  opacity: 0.4;
  cursor: not-allowed;
}

/* card glow on submit */
.card-glow {
  box-shadow: 0 0 0 3px rgba(78,220,216,0.18) !important;
  transition: box-shadow 300ms ease;
}

/* comment list / cards */
.comment-card {
  background: #fff;
  border: 1px solid #f0f0f0;
  border-radius: 25px;
  padding: 12px;
  margin-bottom: 10px;
  box-shadow: 1px 0px 14px rgba(0,0,0,0.2);
  /* DO NOT add a global transition here (we animate .newly-added only) */
}
.comment-card.newly-added {
  opacity: 0;
  transform: translateY(6px);
  transition: opacity 240ms ease, transform 240ms ease;
}

/* card meta and layout */
.comment-card .meta {
  display:flex;
  gap:10px;
  margin-bottom:20px;
}
.comment-card h4 {
  margin: 0 0 6px 0;
  font-size: 15px;
}
.comment-actions {
  margin-top: 8px;
  display:flex;
  gap:8px;
}
.comment-image{
  width: 65px;
  height: 65px;
  border-radius: 50%;
}
.comment-name{
  font-weight:400
}

/* like button */
.btn-like {
  padding:10px 10px 10px 0;
  border: transparent;
  cursor:pointer;
  background: transparent;
  transition: transform 120ms ease, background 120ms ease;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #6b6a6a;
}
.btn-like.pulse {
  transform: scale(1.08);
  box-shadow: 0 6px 14px rgba(0,0,0,0.08);
}
.btn-like[aria-pressed="true"] {
  color: #4edcd8;
}
.btn-like[aria-pressed="false"] {
  color: #6b6a6a;
}
.count-like{
  padding: 0 0 2px 5px;
  font-size: 1em;
}

/* tags */
.tags { 
  margin-top:6px;
  margin-bottom: 20px;
}
.tag {
  display: inline;
  padding: 4px 10px;
  border-radius: 50px;
  font-size: 14px;
  margin-right: 6px;
  border: none;
  font-weight: 500;
  text-align: center;
}
.tag.tag-mbti {
  background: #4edcd8;
  color: #000;
}
.tag.tag-ennea {
  background: #13A6A2;
  color: #fff;
}
.tag.tag-zodiac {
  background: url(static/space.png) no-repeat center center;
  color: #fff;
}

.tag.tag-ennea .wing-icon {
  width: 18px;
  height: auto;
  vertical-align: text-top;
}


/* responsive small adjustments */
@media (max-width: 640px) {
  .comments-header { 
    flex-direction: column; 
    align-items: flex-start; 
    gap: 8px; }
  .vote-btn { 
    align-self: stretch; 
    text-align: center; 
  }
}

